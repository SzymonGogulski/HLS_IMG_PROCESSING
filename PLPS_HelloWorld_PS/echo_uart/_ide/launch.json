{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "tcf-debug",
      "request": "launch",
      "name": "echo_uart_app_hw_1",
      "debugType": "baremetal-zynq",
      "attachToRunningTargetOptions": {
        "targetSetupMode": "standalone",
        "executeScript": true,
        "scriptPath": ""
      },
      "autoAttachProcessChildren": false,
      "target": {
        "targetConnectionId": "Local",
        "peersIniPath": "../../_ide/.peers.ini",
        "context": "zynq"
      },
      "pathMap": [],
      "targetSetup": {
        "resetSystem": true,
        "programDevice": true,
        "partialBitstream": false,
        "skipRevisionCheck": false,
        "device": {
          "plDevice": "Auto Detect",
          "psDevice": "Auto Detect"
        },
        "resetAPU": false,
        "bitstreamFile": "${workspaceFolder}/echo_uart/_ide/bitstream/gauss_1_design.bit",
        "zynqInitialization": {
          "isFsbl": false,
          "usingFSBL": {
            "initWithFSBL": true,
            "fsblFile": "${workspaceFolder}/gauss_1_platform/export/gauss_1_platform/sw/boot/fsbl.elf",
            "fsblExitSymbol": "FsblHandoffJtagExit"
          },
          "usingPs7Init": {
            "runPs7Init": true,
            "runPs7PostInit": true,
            "ps7InitTclFile": "${workspaceFolder}/echo_uart/_ide/psinit/GaussianFilter.tcl"
          }
        },
        "downloadElf": [
          {
            "core": "ps7_cortexa9_0",
            "resetProcessor": true,
            "elfFile": "${workspaceFolder}/echo_uart/build/echo_uart.elf",
            "stopAtEntry": false,
            "isSelfRelocatingApp": false,
            "relativeAddress": ""
          }
        ],
        "crossTriggerBreakpoints": {
          "isSelected": false,
          "breakpoints": []
        }
      },
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}